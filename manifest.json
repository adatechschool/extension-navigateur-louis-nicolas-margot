{
    "manifest_version": 3,
    "name": "Productivity Pet",
    "description": "Hello World",
    "version": "1.0.0",
    "permissions": ["activeTab","tabs"],
    "action": {
        "default_icon": "images/cat_icon.png",
        "default_popup": "index.html"
    },
    "background":
    {
        "service_worker": "background.js"   
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"]
        }
    ]
}